function PublicitaHelp(player) {
var scores = room.getScores();
if (scores.time > 10 && !isTimeAddedShowncinco) {
room.sendAnnouncement("ã€ŒğŸ“£ã€ Â¡ğ—¨ğ˜€ğ—® '!help' ğ—½ğ—®ğ—¿ğ—® ğ˜ƒğ—²ğ—¿ ğ—¹ğ—® ğ—¹ğ—¶ğ˜€ğ˜ğ—® ğ—±ğ—² ğ—°ğ—¼ğ—ºğ—®ğ—»ğ—±ğ—¼ğ˜€ ğ—±ğ—¶ğ˜€ğ—½ğ—¼ğ—»ğ—¶ğ—¯ğ—¹ğ—²ğ˜€!", player, 0xd733ff, "bold", 0);
isTimeAddedShowncinco = true;
}
}

